{
  "name": "generator-spring-boot-application-from-swagger",
  "version": "0.6.0",
  "description": "Yeoman spring boot mvc application generator from swagger api specification",
  "homepage": "https://github.com/mobulum/npm-yo-generator-spring-boot-application-from-swagger",
  "author": {
    "name": "Zenedith",
    "email": "zenedith@wp.pl",
    "url": "http://mobulum.com"
  },
  "files": [
    "generators",
    "generators/app"
  ],
  "main": "generators/index.js",
  "keywords": [
    "swagger",
    "spring",
    "mvc",
    "boot",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "js-yaml": "3.7.0",
    "mkdirp": "0.5.1",
    "pinkie-promise": "2.0.1",
    "swagger-parser": "3.4.1",
    "yeoman-generator": "1.0.1",
    "yosay": "1.2.1"
  },
  "devDependencies": {
    "eslint": "3.14.0",
    "eslint-config-xo-space": "0.15.0",
    "gulp": "3.9.1",
    "gulp-coveralls": "0.1.4",
    "gulp-eslint": "3.0.1",
    "gulp-exclude-gitignore": "1.1.1",
    "gulp-istanbul": "1.1.1",
    "gulp-line-ending-corrector": "1.0.1",
    "gulp-mocha": "3.0.1",
    "gulp-nsp": "2.4.2",
    "gulp-plumber": "1.1.0",
    "yeoman-assert": "2.2.2",
    "yeoman-test": "1.6.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "mocha": true
    },
    "rules": {
      "func-names": [
        "error",
        "always"
      ],
      "no-prototype-builtins": "warn"
    }
  },
  "repository": "git@github.com:mobulum/npm-yo-generator-spring-boot-application-from-swagger.git",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp",
    "test-mocha": "mocha --recursive test/*"
  },
  "license": "MIT"
}
